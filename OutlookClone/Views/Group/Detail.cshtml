@model GroupModel

@{
    ViewData["Title"] = "Outlook Clone - Group detail";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="well well-sm">
                <div class="form-group">
                    <label for="from_id" class="control-label">Group Name</label>
                    <div id="from_id">
                        @Model.GroupName
                    </div>
                </div>

                <div class="form-group">
                    <label for="subject_id" class="control-label">Group Memebers</label>
                    <div id="subject_id">
                        <ul>
                            @foreach (var member in @Model.GroupMembers)
                            {
                                <li>member.FullName</li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>